{
  "name": "social-network",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm start --prefix api",
    "build": "npm run build --prefix frontend",
    "dev": "concurrently \"npm run dev --prefix api\" \"npm start --prefix frontend\""
  },
  "workspaces": [
    "api",
    "frontend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dimimikadze/create-social-network.git"
  },
  "bugs": {
    "url": "https://github.com/dimimikadze/create-social-network/issues"
  },
  "engines": {
    "node": ">=8.10"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css,html}": [
      "prettier --write '**/*.js'"
    ]
  },
  "license": "MIT"
}
